{
  "private": true,
  "name": "starter-server",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "git@github.com:MatejBransky/starter-server.git",
  "author": "Matej Bransky <matej.bransky@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "build": "tsc && node ./build/index.js"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.11.7",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.4000"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write",
      "eslint --ext ts --ext js",
      "git add"
    ],
    "{*.{json,md}}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
